  "author": "Kamil <kamil@essekkat.pl> (https://essekkat.pl)",
  "license": "Unlicense",
  "private": true,
  "description": "Export Kindle Vocabulary Builder into markdown files",
  "scripts": {
    "clean": "rimraf dist/ ./kindle-vocab-md-0.?.?.tgz out/",
    "build": "node --no-warnings esbuild.config.mjs",
    "test": "jest --clearCache && jest .",
    "lint": "prettier --check \"src/**/*.ts\" && eslint --fix-dry-run ./src && tsc ",
    "prepare": "npm test && npm run lint && npm run build && husky install",
    "preversion": "npm test && npm run lint",
    "main": "npm run build && node dist/main.js"
  },
  "keywords": [],
  "files": [
    "./dist/*"
  ],
  "main": "dist/index.js",
  "devDependencies": {
    "@commander-js/extra-typings": "^10.0.1",
    "@jest/globals": "^29.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.18",
    "@types/nunjucks": "^3.2.1",
    "@types/sqlite3": "^3.1.8",
    "@types/unidecode": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "esbuild": "^0.17.17",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "prettier": "^2.8.3",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "gray-matter": "^4.0.3",
