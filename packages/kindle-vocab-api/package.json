{
  "name": "@ekamil/kindle-vocab-api",
  "version": "0.1.0",
  "author": "Kamil <kamil@essekkat.pl> (https://essekkat.pl)",
  "license": "Unlicense",
  "description": "API interface for Kindle Vocabulary Builder DB file",
  "scripts": {
    "clean": "rimraf dist/",
    "build-tsc": "rimraf dist/ && tsc --noEmit false",
    "build": "rimraf dist/ && node --no-warnings esbuild.config.mjs && tsc --emitDeclarationOnly true",
    "test": "jest ./src",
    "lint": "prettier --check \"src/**/*.ts\" && eslint ./src && tsc --noEmit true",
    "preversion": "npm test && npm run lint",
    "main": "npm run build-tsc && node dist/demo.js"
  },
  "keywords": [],
  "files": [
    "./dist/*"
  ],
  "exports": "./dist/main.js",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "private": false,
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.18",
    "@types/sqlite3": "^3.1.8",
    "@types/unidecode": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "esbuild": "^0.17.17",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "sqlite3": "^5.1.4",
    "unidecode": "^0.1.8"
  },
  "engines": {
    "node": ">=14.16"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
