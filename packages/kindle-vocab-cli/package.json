  "author": "Kamil <kamil@essekkat.pl> (https://essekkat.pl)",
  "license": "Unlicense",
  "description": "CLI tool to export Kindle Vocabulary Builder database",
  "scripts": {
    "clean": "rimraf dist/ out/",
    "build": "rimraf dist/ out/ && node --no-warnings esbuild.config.mjs",
    "test": "jest ./src",
    "lint": "prettier --check \"src/**/*.ts\" && eslint ./src && tsc ",
    "preversion": "npm test && npm run lint",
    "main": "npm run build && node dist/main.js"
  },
  "keywords": [],
  "files": [
    "./dist/*"
  ],
  "exports": "./dist/main.js",
  "main": "./dist/main.js",
  "bin": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "private": false,
  "type": "module",
  "devDependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@types/nunjucks": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "esbuild": "^0.17.18",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@ekamil/kindle-vocab-api": "^1.2.1",
    "commander": "^10.0.1",
    "gray-matter": "^4.0.3",
    "nunjucks": "^3.2.4",
    "unidecode": "^0.1.8"
  },
  "engines": {
    "node": ">=14.16"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
